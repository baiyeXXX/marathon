#%RAML 1.0 Library
uses:
  strings: stringTypes.raml

types:
  Seccomp:
    type: object
    properties:
      profileName?:
        type: string
        description: Filename of the Seccomp profile found on the agent.  The task will fail if that file does not exist.
      unconfined?:
        type: boolean
        description:  Set to `true` if Seccomp is not applied to the container
  LinuxInfo:
    type: object
    properties:
      seccomp?: Seccomp
